@page
@model ContosoUniversity.Pages.AboutModel
@{
    ViewData["Title"] = "Student Body Statistics";
}

<h2>Student Body Statistics</h2>

<table class="table table-bordered">
    <tr>
        <th>@Html.DisplayNameFor(m => Model.Students[0].EnrollmentDate)</th>
        <th>@Html.DisplayNameFor(m => Model.Students[0].StudentCount)</th>
    </tr>
    @foreach (var item in Model.Students)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.EnrollmentDate)</td>
            <td>@Html.DisplayFor(m => item.StudentCount)</td>
        </tr>
     }
</table>
